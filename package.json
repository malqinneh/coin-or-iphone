{
  "name": "coin-or-iphone",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "bankai build index.js --html [ --favicon assets/favicon.ico --head \"$(cat meta.html)\" ] && cp _redirects dist/_redirects",
