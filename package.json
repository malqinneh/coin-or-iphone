    "start": "bankai start index.js --html [ --favicon assets/favicon.ico ]",
    "format": "prettier --write '**/*.js'"
  },
  "dependencies": {
    "choo": "^6.0.0",
    "choo-devtools": "^1.3.1",
    "sheetify": "^6.1.1",
    "tinytime": "^0.2.6"
  },
  "devDependencies": {
    "@rosszurowski/vanilla": "^1.3.2",
    "bankai": "^8.1.1",
    "hibiscss": "^0.0.8",
    "prettier": "^1.7.4"
  },
  "prettier": {
    "singleQuote": true
  },
  "license": "MIT"
}
